                    <div style="display: flex;border-bottom: 1px solid gray;">
                        <img src="../assets/navbarLogo.png" style="width: 200px;margin-left: 120px;">
                        <button onclick="DongDangnhap()" type="button" class="close closs-css" aria-label="Close" style="background-color: white;font-size: 38px;margin-left: 18%;border: none;">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div style="display: flex;font-size: 25px;text-align: center;line-height: 80px;font-weight: bolder;">
                        <div id="txt_dn" onclick="switchToLogin()" style="border-bottom: 2px solid #B32A45;width: 180px;margin-left: 45px;">Đăng nhập</div>
                        <div id="txt_dk" onclick="switchToSignup()" style="border-bottom: 2px solid gray;width: 180px;">Đăng ký</div>
                    </div>
                    <br>
                    <br>
        
                    <div id="Dangnhap" class="form active">
                        <form>
                            <label for="email1" style="font-weight: bolder; margin-left: 45px;" class="User">Email</label>
                            <br>
                            <input id="email1" style="border-radius: 7px; height: 40px; width: 353px; margin-left: 45px;" type="email" placeholder="Email">
                            <br><br>
                            <label for="password1" style="font-weight: bolder; margin-left: 45px;" class="User">Mật khẩu</label>
                            <br>
                            <input id="password1" style="border-radius: 7px; height: 40px; width: 353px; margin-left: 45px;" type="password" placeholder="Mật khẩu">
                            <br><br><br>
                            <button type="button" onclick="Login()" style="background-color: red; color: white; border-radius: 7px; height: 40px; width: 359px; margin-left: 45px; font-weight: bolder; font-size: 20px;">Đăng nhập</button>
                        </form>
                        <p style="margin-left: 45px;">Chưa có tài khoản? <span onclick="switchToSignup()" style="color: blue; cursor: pointer;">Đăng ký</span></p>
                    </div>
                    <div id="Dangky" class="form">
                        <form>
                            <label for="email2" style="font-weight: bolder; margin-left: 45px;" class="User">Email</label>
                            <br>
                            <input id="email2" style="border-radius: 7px; height: 40px; width: 353px; margin-left: 45px;" type="email" placeholder="Email">
                            <br><br>
                            <label for="password2" style="font-weight: bolder; margin-left: 45px;" class="User">Mật khẩu</label>
                            <br>
                            <input id="password2" style="border-radius: 7px; height: 40px; width: 353px; margin-left: 45px;" type="password" placeholder="Mật khẩu">
